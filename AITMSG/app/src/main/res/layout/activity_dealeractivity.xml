<?xml version="1.0" encoding="utf-8"?>




<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/voiler"
    tools:context=".dealeractivity">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


           <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_marginTop="66dp"
        android:layout_marginBottom="601dp"
        android:fontFamily="cursive"
        android:text="Loading..."
        android:textColor="@color/red"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.spark.submitbutton.SubmitButton
        android:id="@+id/submitButton4"
        android:layout_width="200dp"

        android:layout_height="200dp"
        android:layout_marginTop="193dp"
        android:layout_marginBottom="546dp"
        android:onClick="UserList"
        android:text="USER LIST"
        android:textColor="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:sub_btn_background="@color/white"
        app:sub_btn_duration="3000"
        app:sub_btn_line_color="@color/red"
        app:sub_btn_ripple_color="@color/red"
        app:sub_btn_tick_color="@color/white" />

    <com.spark.submitbutton.SubmitButton
        android:id="@+id/submitButton2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="554dp"
        android:layout_marginBottom="185dp"
        android:onClick="web"
        android:text="VISIT WEBSITE"
        android:textColor="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/submitButton"
        app:layout_constraintTop_toTopOf="parent"
        app:sub_btn_background="@color/white"
        app:sub_btn_duration="3000"
        app:sub_btn_line_color="@color/red"
        app:sub_btn_ripple_color="@color/red"
        app:sub_btn_tick_color="@color/white" />

    <com.spark.submitbutton.SubmitButton
        android:id="@+id/submitButton3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="429dp"
        android:layout_marginBottom="310dp"
        android:onClick="add"
        android:text="ADD USERS"
        android:textColor="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:sub_btn_background="@color/white"
        app:sub_btn_duration="3000"
        app:sub_btn_line_color="@color/red"
        app:sub_btn_ripple_color="@color/red"
        app:sub_btn_tick_color="@color/white" />

    <com.spark.submitbutton.SubmitButton
        android:id="@+id/submitButton"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="311dp"
        android:layout_marginBottom="428dp"
        android:onClick="msg"
        android:text="WELCOME MSG"
        android:textColor="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:sub_btn_background="@color/white"
        app:sub_btn_duration="3000"
        app:sub_btn_line_color="@color/red"
        app:sub_btn_ripple_color="@color/red"
        app:sub_btn_tick_color="@color/white" />

    <Button
        android:id="@+id/button2"
        android:layout_width="92dp"
        android:layout_height="83dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="19dp"
        android:background="@drawable/btn_background"
        android:onClick="Group"
        android:text="ADD GROUP"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/submitButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/submitButton3"
        app:layout_constraintTop_toBottomOf="@+id/submitButton">

    </Button>

    <Button
        android:id="@+id/button5"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/btn_background"
        android:onClick="grouplist"
        android:text="Group List"

        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/submitButton3"
        app:layout_constraintEnd_toStartOf="@+id/submitButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/submitButton4" />

    <Button
        android:id="@+id/button7"
        android:layout_width="103dp"
        android:layout_height="88dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="39dp"
        android:layout_marginRight="39dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/btn_background"
        android:onClick="useredit"
        android:text="Edit User Details"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/submitButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/submitButton4"

        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button8"
        android:layout_width="82dp"
        android:layout_height="75dp"

        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="137dp"
        android:background="@drawable/btn_background"
        android:onClick="bday"
        android:text="Birthday/Festive MSgs"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/submitButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/submitButton3" />
    </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>