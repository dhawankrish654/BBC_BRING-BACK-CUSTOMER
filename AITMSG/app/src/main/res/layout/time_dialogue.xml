<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="700dp">

    <EditText
        android:id="@+id/low"
        android:layout_width="82dp"
        android:layout_height="97dp"


        android:layout_marginStart="165dp"
        android:layout_marginLeft="165dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="165dp"
        android:layout_marginRight="165dp"
        android:layout_marginBottom="543dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/red"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addlow"
        android:layout_width="87dp"
        android:layout_height="68dp"

        android:layout_marginStart="78dp"
        android:layout_marginLeft="78dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="560dp"
        android:background="@drawable/btn_background"
        android:text="+"
        android:textColor="@color/white"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/low"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/sublow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="72dp"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:layout_marginBottom="561dp"
        android:background="@drawable/btn_background"
        android:text="-"
        android:textColor="@color/white"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/low"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/high"
        android:layout_width="82dp"
        android:layout_height="97dp"

        android:layout_marginStart="164dp"
        android:layout_marginLeft="164dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="165dp"
        android:layout_marginRight="165dp"
        android:layout_marginBottom="358dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/red"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/low" />

    <Button
        android:id="@+id/addhigh"

        android:layout_width="89dp"
        android:layout_height="70dp"

        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="370dp"
        android:background="@drawable/btn_background"
        android:text="+"
        android:textColor="@color/white"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/high"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addlow" />

    <Button
        android:id="@+id/subhigh"


        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="122dp"
        android:layout_marginEnd="77dp"
        android:layout_marginRight="77dp"
        android:layout_marginBottom="373dp"
        android:background="@drawable/btn_background"
        android:text="-"
        android:textColor="@color/white"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/high"
        app:layout_constraintTop_toBottomOf="@+id/sublow" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="171dp"
        android:layout_marginLeft="171dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="171dp"
        android:layout_marginRight="171dp"
        android:text="FROM"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/low"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="181dp"
        android:layout_marginLeft="181dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="181dp"
        android:layout_marginRight="181dp"
        android:text="TOO"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/high"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/low" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="336dp"
        android:layout_height="161dp"


        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="87dp"
        android:text=":-NOTE-Both Cannot Be Equal \n Time is in hours"
        android:textAlignment="center"
        android:textColor="@color/green"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/high" />

</androidx.constraintlayout.widget.ConstraintLayout>